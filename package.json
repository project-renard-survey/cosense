{
  "name": "rapid-sensemaking-framework",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "electron": "^1.7.9",
    "react-scripts": "^3.2.0",
    "foreman": "^3.0.1"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "electron-log": "^3.0.8",
    "electron-packager": "^14.1.0",
    "fbp-client": "^0.4.0",
    "fbp-graph": "^0.4.0",
    "fix-path": "^2.1.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-router-dom": "^5.1.2",
    "socket.io-client": "^2.3.0",
    "typescript": "^3.7.2"
  },
  "homepage": "./",
  "main": "electron/electron-only/electron-starter.js",
  "scripts": {
    "start": "nf start -p 3000",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-start": "node electron/electron-only/electron-wait-react",
    "react-start": "BROWSER=none react-scripts start",
    "tswatch-electron": "tsc --build tsconfig-electron.json --watch",
    "tsbuild-electron": "tsc --build tsconfig-electron.json",
    "tswatch-react": "tsc --build tsconfig-react.json --watch",
    "tsbuild-react": "tsc --build tsconfig-react.json",
    "package-mac": "electron-packager . RSF --platform=darwin --overwrite",
    "package-win": "electron-packager . RSF --platform=win32 --overwrite"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
